<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="root" prefHeight="750.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.menu.MeniController">
    <children>
      <MenuBar fx:id="MainMenu" layoutX="1.0" layoutY="2.0" prefHeight="29.0" prefWidth="654.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <menus>
  <Menu fx:id="game" mnemonicParsing="false" text="Game ">
  
               <items>
  
            <MenuItem fx:id="start" mnemonicParsing="false" onAction="#startGame" text="Start" />
                  <MenuItem fx:id="undo" mnemonicParsing="false" onAction="#undoMove" text="Undo" />  
            <MenuItem fx:id="exit" mnemonicParsing="false" onAction="#exit" text="Exit" />  
               </items>
  
          </Menu>           
  <Menu fx:id="Leaderboard" mnemonicParsing="false" text="Leaderboard ">
  
               <items>
  
            <MenuItem fx:id="allTheTime" mnemonicParsing="false" onAction="#viewAllTheTime" text="All the time" />  
            <MenuItem fx:id="top10" mnemonicParsing="false" onAction="#viewTop10" text="Top 10" />  
               </items>
  
          </Menu>           
  <Menu fx:id="about" mnemonicParsing="false" text="About ">
  
               <items>
  
            <MenuItem fx:id="aboutGame" mnemonicParsing="false" onAction="#viewAboutGame" text="About game" />  
            <MenuItem fx:id="aboutAuthor" mnemonicParsing="false" onAction="#viewAboutAuthor" text="About author" />  
               </items>
  
          </Menu>           
        </menus>
      </MenuBar>
      <StackPane fx:id="stackPane" layoutY="31.0" prefHeight="720.0" prefWidth="654.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
    </children>
</AnchorPane>
